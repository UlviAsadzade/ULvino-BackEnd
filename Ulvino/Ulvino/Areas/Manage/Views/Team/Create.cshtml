@model Team
@{
    ViewData["Title"] = "Create";
}

<div class="container-fluid pt-3">
    <h1 class="pb-3 text-primary">Create</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="form-group col-sm-6 ">
                <label class="font-18" asp-for="FullName">FullName</label>
                <input asp-for="FullName" class="form-control input-rounded" placeholder="FullName">
                <span class="text-danger" asp-validation-for="FullName"></span>
            </div>
            <div class="form-group col-md-6 ">
                <label class="font-18" asp-for="Profession">Profession</label>
                <input asp-for="Profession" class="form-control input-rounded" placeholder="Profession">
                <span class="text-danger" asp-validation-for="Profession"></span>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label class="font-18" asp-for="FacebookUrl">FacebookUrl</label>
                <input asp-for="FacebookUrl" class="form-control input-rounded" placeholder="FacebookUrl">
                <span class="text-danger" asp-validation-for="FacebookUrl"></span>
            </div>
            <div class="form-group col-md-6">
                <label class="font-18" asp-for="TwitterUrl">TwitterUrl</label>
                <input asp-for="TwitterUrl" class="form-control input-rounded" placeholder="TwitterUrl">
                <span class="text-danger" asp-validation-for="TwitterUrl"></span>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label class="font-18" asp-for="GooglePlusUrl">GooglePlusUrl</label>
                <input asp-for="GooglePlusUrl" class="form-control input-rounded" placeholder="GooglePlusUrl">
                <span class="text-danger" asp-validation-for="GooglePlusUrl"></span>
            </div>
            <div class="form-group col-md-6">
                <label class="font-18" asp-for="PinterestUrl">PinterestUrl</label>
                <input asp-for="PinterestUrl" class="form-control input-rounded" placeholder="PinterestUrl">
                <span class="text-danger" asp-validation-for="PinterestUrl"></span>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label class="font-18" asp-for="ImageFile">ImageFile</label>
                <input asp-for="ImageFile" class="form-control input-rounded" placeholder="ImageFile">
                <span class="text-danger" asp-validation-for="ImageFile"></span>
            </div>
        </div>
        <div class="form-group pt-5 text-center">
            <button type="submit" class="btn btn-primary btn-rounded font-20 w-25 text-center">Create</button>
        </div>

    </form>
</div>



