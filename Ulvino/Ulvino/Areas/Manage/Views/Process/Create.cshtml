@model Process
@{
    ViewData["Title"] = "Create";
}

<div class="container-fluid pt-3">
    <h1 class="pb-3 text-primary">Create</h1>
    <form method="post">
        <div class="row">
            <div class="form-group col-sm-6 ">
                <label class="font-18" asp-for="Title">Title</label>
                <input asp-for="Title" class="form-control input-rounded" placeholder="Title">
                <span class="text-danger" asp-validation-for="Title"></span>
            </div>
            <div class="form-group col-md-6 ">
                <label class="font-18" asp-for="Icon">Icon</label>
                <input asp-for="Icon" class="form-control input-rounded" placeholder="Icon">
                <span class="text-danger" asp-validation-for="Icon"></span>
            </div>
        </div>

        <div class="form-group pt-5 text-center">
            <button type="submit" class="btn btn-primary btn-rounded font-20 w-25 text-center">Create</button>
        </div>

    </form>
</div>



